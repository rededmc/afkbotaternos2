{
    "logs": {
        "log-chat-to-console": true,
        "log-chat-to-file": true
    },

    "server": {
        "host": "localhost",
        "port": 25565
    },

    "afkbot": {
        "username": "AFKBot",
        "password": "",
        "auth-method": "mojang"
    },

    "discord": {
        "token": "",
        "guild-id": "",
        "channel-id": "",
        "embed-hex-color": "#000ff",
        "prefix": "+",

        "bot-rpc": {
            "enable": true,
            "text": "Being AFK is my profession"
        },

        "send-chat-to-minecraft": false
    },

    "auto-login": {
        "enable": false,
        "password": "",
        "repeat-password-on-register": true
    },

    "whispers": {
        "enable-answer": true,
        "timeout-on-whisper": 1000,
        "message-to-answer": "Sorry, I'm an AFKBot"
    },

    "pvp": {
        "enable": true,
        "attack-players": false,
        "attack-endermans": false
    },

    "auto-eat": {
        "enable": true,
        "send-status": false,
        "priority": "foodPoints",
        "start-at": 19,
        "banned-foods": [
            "rotten_flesh",
            "poisonous_potato",
            "pufferfish"
        ]
    },

    "bloodhound": {
        "enable": true
    },

    "viewer": {
        "enable": true,
        "first-person": false,
        "view-distance": 6,
        "port": 3000
    },

    "low-health": {
        "warn-on-low-health": false,
        "health-points": 5,
        "disconnect-on-low-health": false
    },

    "message-on-interval": {
        "enable": false,
        "interval": 10000,
        "message": "I am an AFKBot"
    },

    "windows-notifications": {
        "on-banned": true,
        "on-kicked": true,
        "on-death": true
    },

    "pathfind": {
        "pathfind-range": 1.5,
        "max-dropdown-blocks": 4
    },

    "misc-options": {
        "send-chat-to-ds": false,
        "antikick-jump": true,
        "look-entities": true
    },

    "timeouts": {
        "on-kicked": 5000,
        "antikick-jump-interval": 600000
    },

    "debug": false,
    "check-updates": true
}